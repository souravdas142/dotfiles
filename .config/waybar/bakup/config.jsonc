      "tooltip-format-ethernet": "<span foreground='#7daea3'>ÔîÇ </span>{ifname} via {gwaddr}",
	  //"tooltip-format": "{ifname} via {gwaddr} Û∞äó",
      //"tooltip-format-wifi": "Ôá´ {ifname}\n{essid} ({signalStrength}%)\n{ipaddr} via {gwaddr}",
