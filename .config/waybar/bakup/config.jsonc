    "battery": {
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 20
        },
       "full-at": 100,
       "interval": 5,
        "format": "{icon} {capacity}%",
        "format-charging": "ÔÉß {capacity}%",
        //"format-plugged": "Ôá¶ {capacity}%",
        "format-time": "{H}h {M}min",
